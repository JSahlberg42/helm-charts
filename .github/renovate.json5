{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:base',
    ':dependencyDashboard',
  ],
  dependencyDashboardAutoclose: true,
  automergeType: 'pr',
  platformAutomerge: true,
  rebaseWhen: 'conflicted',
  "timezone": "Europe/Paris",
  "schedule": [
    "before 9pm on friday"
  ],
  prConcurrentLimit: 0,
  prHourlyLimit: 0,
  terragrunt: {
    enabled: false
  },
  labels: [
    'dependencies',
  ],
  vulnerabilityAlerts: {
    enabled: true,
    labels: [
      'security',
    ],
  },
  patch: {
    automerge: true,
  },
  pin: {
    automerge: true,
  },
  regexManagers: []
}